{
  "name": "reactiac",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "../"
  },
  "postStartCommand": "pnpm install --ignore-scripts --offline --frozen-lockfile",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "github.vscode-github-actions",
        "Orta.vscode-jest",
        "biomejs.biome",
        "reduckted.vscode-gitweblinks"
      ]
    }
  }
}
