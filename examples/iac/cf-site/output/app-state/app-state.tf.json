{
  "terraform": {
    "required_providers": {
      "aws": {
        "source": "aws",
        "version": "5.63.1"
      }
    },
    "backend": {
      "s3": {
        "bucket": "reactiac-website-tf-state",
        "key": "tfstates/app-state.tfstate",
        "region": "us-east-1",
        "use_lockfile": true
      }
    }
  },
  "provider": {
    "aws": [
      {
        "default_tags": [
          {
            "tags": {
              "iac:stack-title": "ReactIAC Website",
              "iac:stack-name": "reactiac-website"
            }
          }
        ],
        "region": "us-east-1"
      }
    ]
  },
  "resource": {
    "aws_s3_bucket": {
      "aws_s3_bucket_tfstate": {
        "bucket": "reactiac-website-tf-state"
      }
    },
    "aws_s3_bucket_versioning": {
      "aws_s3_bucket_versioning_enableversioning": {
        "bucket": "${aws_s3_bucket.aws_s3_bucket_tfstate.id}",
        "versioning_configuration": {
          "status": "Enabled"
        }
      }
    }
  }
}