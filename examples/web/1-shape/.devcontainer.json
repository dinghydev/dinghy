{
  "name": "1-shape",
  "runArgs": [
    "--name",
    "1-shape"
  ],
  "image": "public.ecr.aws/f2v6q7q7/reactiac/reactiac:latest-linux-arm64",
  "containerEnv": {
    "AWS_PROFILE": "reactiac",
    "APP_HOME": "/reactiac/workspace/project/app"
  },
  "mounts": [
    "source=/Users/guo/.ssh,target=/root/.ssh,type=bind",
    "source=/Users/guo/.npmrc,target=/root/.npmrc,type=bind"
  ],
  "workspaceMount": "source=/Users/guo/github/reactiac/reactiac/examples/web/1-shape,target=/reactiac/workspace/project/app,type=bind",
  "workspaceFolder": "/reactiac/workspace/project",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "denoland.vscode-deno"
      ],
      "settings": {
        "remote.extensionKind": {
          "ms-azuretools.vscode-docker": [
            "ui"
          ],
          "denoland.vscode-deno": [
            "workspace"
          ]
        }
      }
    }
  }
}