{
  "name": "@reactiac/root",
  "version": "0.1",
  "description": "a React DX with programable diagrams for any IaC",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "show-info": "export;git rev-parse HEAD;git rev-list --count HEAD",
    "aws-login": "aws codeartifact login --tool npm --repository test-repo --domain reactiac-test-repo --domain-owner 313225491971 --namespace @reactiac --region eu-west-1",
    "release-version-update": "node --experimental-strip-types -p \"require('./scripts/releaseVersion.ts').updateVersion()\"",
    "release-version-reset": "node --experimental-strip-types -p \"require('./scripts/releaseVersion.ts').resetVersion()\"",
    "docker-publish": "node --experimental-strip-types -p \"require('./scripts/dockerPublish.ts').publish('313225491971.dkr.ecr.eu-west-1.amazonaws.com/test/cli')\"",
    "clean": "rm -rf node_modules *.tgz */*/node_modules  */*/dist */*/tsconfig.tsbuildinfo */*/*.tgz */*/oclif.manifest.json"
  },
  "files": [
    "dist"
  ],
  "private": true,
  "keywords": [
    "React",
    "Architecture",
    "Diagrams",
    "iac",
    "OpenTufu",
    "Terraform",
    "Draw.io"
  ],
  "author": "Guo Du",
  "license": "BSL-1.1",
  "devDependencies": {
    "@types/node": "^20.17.24",
    "@types/mocha": "^10"
  },
  "packageManager": "pnpm@10.6.3"
}
